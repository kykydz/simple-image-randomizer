<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.file.filemanager.PhotoController">
    <children>
        <Button fx:id="uploadButton" layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleUploadButton" text="Upload" />
        <Button fx:id="saveButton" disable="true" layoutX="127.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
        <Button fx:id="showButton" disable="true" layoutX="240.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleShowButton" text="Show Random" />
        <ImageView fx:id="imageView1" fitHeight="184.0" fitWidth="212.0" layoutX="14.0" layoutY="67.0" pickOnBounds="true" preserveRatio="true" />
        <ImageView fx:id="imageView2" fitHeight="200.0" fitWidth="212.0" layoutX="219.0" layoutY="67.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
